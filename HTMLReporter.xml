    </test>
    <test name="Integration Settings">
        <packages>
            <package name="test.CareNetSettings.IntegrationSettings"/>
        </packages>
    </test>
    <test name="Medical Providers">
        <packages>
            <package name="test.CareNetSettings.MedicalProviders"/>
        </packages>
